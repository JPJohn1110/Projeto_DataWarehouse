    <sql>SELECT  productname, pl.productline, productscale, orderdate,
e.jobtitle, c.country, c.state, c.city, o.ordernumber as nota, 
c.customernumber as cliente,quantityordered AS quantidade, priceeach AS valor, e.employeenumber
 
FROM _orders o
inner JOIN _customers c ON o.customernumber = c.customernumber
inner JOIN _orderdetails od ON o.ordernumber = od.ordernumber
inner JOIN _products p ON od.productcode = p.productcode
inner JOIN _employees e ON CAST(c.salesrepemployeenumber AS smallint) = e.employeenumber
inner JOIN _payments pay ON o.customernumber = pay.customernumber
inner JOIN _offices off ON e.officecode = off.officecode 
JOIN _productlines pl ON p.productline = pl.productline

</sql>
